{
  "name": "single-spa-angular",
  "version": "3.5.0",
  "description": "Helpers for building single-spa applications which use Angular 2",
  "main": "lib/browser-lib/cjs/single-spa-angular.js",
  "module": "lib/browser-lib/esm/single-spa-angular.js",
  "types": "lib/browser-lib/single-spa-angular.d.ts",
  "builders": "builders.json",
  "schematics": "./schematics.json",
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "build": "npm run clean && npm run node && npm run parcel && npm run spa && npm run :copy-schematic-files",
    "clean": "rimraf lib parcel",
    "test": "jest",
    "lint": "eslint ./src/**/*.ts",
    ":copy-schematic-files": "cpx \"src/**/_files/**/**\" lib",
    "parcel": "ngc -p tsconfig.parcel.json",
    "spa": "rollup -c rollup.config.js",
    "node": "tsc -p tsconfig.node.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/single-spa/single-spa-angular.git"
  },
  "keywords": [
    "single-spa",
    "angular"
  ],
  "author": "Joel Denning",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/single-spa/single-spa-angular/issues"
  },
  "homepage": "https://github.com/single-spa/single-spa-angular#readme",
  "peerDependencies": {
    "@angular-devkit/architect": ">=0.10.0",
    "@angular-devkit/build-angular": ">=0.10.0",
    "@angular-devkit/core": ">=0.10.0",
    "@angular/compiler": ">=2",
    "@angular/compiler-cli": ">=2",
    "@angular/core": ">=2",
    "rxjs": ">=6.0.0",
    "webpack": ">=4.6.0",
    "webpack-merge": "^4.2.1",
    "semver": ">=6.2.0",
    "single-spa": ">=4.0.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.13.6",
    "@angular-devkit/build-angular": "^0.13.6",
    "@angular-devkit/core": "^7.3.6",
    "@angular-devkit/schematics": "^7.3.6",
    "@angular/common": "^7.2.16",
    "@angular/compiler": "^7.2.16",
    "@angular/compiler-cli": "^7.2.9",
    "@angular/core": "^7.2.16",
    "@angular/platform-browser": "^7.2.16",
    "@schematics/angular": "^7.3.6",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.3",
    "@types/webpack-merge": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "cpx": "^1.5.0",
    "eslint": "^5.15.2",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "jest": "^24.5.0",
    "rimraf": "^2.6.2",
    "rollup": "^2.10.7",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^24.0.0",
    "typescript": "3.2.4",
    "webpack": "^4.29.6"
  },
  "dependencies": {
    "single-spa": ">= 5.1.1"
  }
}
